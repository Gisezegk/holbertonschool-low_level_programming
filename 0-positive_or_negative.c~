#include <stdlib.h>
#include <time.h>
#include <studio.h>
/**
 * main - compiled
 * Return: always 0
 */

int main(void)
{
	int n;

	srand(time(0));
	n = rand() - RAND_MAX / 2;
	if (n < 0)
	  {
	    is positive
	      }
	printf("n is greater than 0\n");
} else if (n = 0) {
  is zero
  }
printf("n is equal than 0\n");

 else if (n > 0) {
   is negative
     }
printf("n is less than 0\n");
return (0);
}
